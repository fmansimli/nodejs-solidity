{
  "presets": ["@babel/preset-env", "minify"],
  "plugins": [
    ["@babel/transform-runtime"],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".js", ".ts", ".json"],
        "alias": {
          "@src": "./src",
          "test": ["./test/"]
        }
      }
    ]
  ]
}
